<UserControl x:Class="LeftNavigationPanel.LeftNavigationPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LeftNavigationPanel"
             mc:Ignorable="d" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="praca" Content="Praca.by" Grid.Row="0" Grid.RowSpan="1" Click="NavButton_Click"/>
        <Button x:Name="math" Content="Online calculator" Grid.Row="1" Grid.RowSpan="1" Click="NavButton_Click"/>
        <Button x:Name="google" Content="Google" Grid.Row="2" Grid.RowSpan="1" Click="NavButton_Click"/>
        <Button x:Name="translate" Content="Translator" Grid.Row="3" Grid.RowSpan="1" Click="NavButton_Click"/>
        <Button x:Name="news" Content="News" Grid.Row="4" Grid.RowSpan="1" Click="NavButton_Click"/>
    </Grid>
</UserControl>
